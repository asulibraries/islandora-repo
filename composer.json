{
    "name": "islandora/drupal-project",
    "description": "Project template for Drupal 8 projects with composer",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Islandora Foundation",
            "role": "Owner",
            "email": "community@islandora.ca"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        },
        {
            "type": "package",
            "package": {
                "name": "d3/d3",
                "version": "v3.5.17",
                "type": "drupal-library",
                "source": {
                    "url": "https://github.com/d3/d3.git",
                    "type": "git",
                    "reference": "tags/v3.5.17"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "highlightjs/highlightjs",
                "version": "9.7.0",
                "type": "drupal-library",
                "source": {
                    "url": "https://github.com/highlightjs/highlight.js.git",
                    "type": "git",
                    "reference": "tags/9.7.0"
                }
            }
        }
    ],
    "require": {
        "php": "^7.4||^8.0",
        "bower-asset/jquery-ui-slider-pips": "^1.11",
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.6.5",
        "d3/d3": "^3.5",
        "drupal/address": "^1.9",
        "drupal/admin_toolbar": "^3.1",
        "drupal/advagg": "4.x-dev@dev",
        "drupal/archivesspace": "^1.1",
        "drupal/auto_entitylabel": "^3.0@beta",
        "drupal/bootstrap_barrio": "5.1.5",
        "drupal/bootstrap_sass": "^5.0",
        "drupal/captcha": "^1.2",
        "drupal/cas": "^2.0@beta",
        "drupal/cas_attributes": "^2.0@beta",
        "drupal/cdn": "^3.5",
        "drupal/clamav": "^2.0",
        "drupal/config_filter": "^2.2",
        "drupal/config_split": "^2.0.0-beta2",
        "drupal/console": "~1.0",
        "drupal/content_browser": "^1.0@alpha",
        "drupal/context": "^4.0@beta",
        "drupal/contextual_range_filter": "^1.0",
        "drupal/core-composer-scaffold": "^9.1",
        "drupal/core-dev": "^9.1",
        "drupal/core-project-message": "^9.1",
        "drupal/core-recommended": "^9.1",
        "drupal/devel": "^5",
        "drupal/diff": "^1.0",
        "drupal/embed": "^1.3",
        "drupal/entity_embed": "^1.0@RC",
        "drupal/entity_reference_unpublished": "^1.2",
        "drupal/facets": "1.x-dev@dev",
        "drupal/features": "^3.11",
        "drupal/file_replace": "^1.1",
        "drupal/filehash": "^1.6",
        "drupal/filelog": "^2.0",
        "drupal/flexslider": "^2.0",
        "drupal/flysystem": "^2.0@alpha",
        "drupal/format_bytes": "^2",
        "drupal/geolocation": "^3.4",
        "drupal/getjwtonlogin": "^2.0",
        "drupal/google_analytics": "^3.1",
        "drupal/group": "^1.3",
        "drupal/honeypot": "^2.0",
        "drupal/inline_entity_form": "^1.0@RC",
        "drupal/jsonb": "^1.0@beta",
        "drupal/jw_player": "^1.0@beta",
        "drupal/jwt": "^1.0@beta",
        "drupal/masquerade": "^2.0@beta",
        "drupal/matomo": "^1.7",
        "drupal/message": "^1.0",
        "drupal/message_subscribe_email": "^1.0",
        "drupal/metatag": "1.16",
        "drupal/metatag_google_scholar": "^1.0",
        "drupal/migrate_devel": "^2.0",
        "drupal/migrate_file": "^2.0",
        "drupal/migrate_plus": "^5.2",
        "drupal/migrate_process_s3": "2.0.x-dev@dev",
        "drupal/migrate_source_ui": "^1.0.0-rc2",
        "drupal/migrate_tools": "^5.1",
        "drupal/paragraphs": "^1.11",
        "drupal/pathauto": "^1.8",
        "drupal/pdf": "1.x-dev",
        "drupal/rdfui": "^1.0-beta1",
        "drupal/recaptcha_v3": "^1.6",
        "drupal/redirect": "^1.6",
        "drupal/redis": "^1.5",
        "drupal/rest_oai_pmh": "^2",
        "drupal/restui": "^1.16",
        "drupal/rrssb": "^2.3",
        "drupal/s3fs": "^3.0@beta",
        "drupal/s3fs_cors": "^1.0@beta",
        "drupal/schema_metatag": "^2.2",
        "drupal/search_api": "^1.23",
        "drupal/search_api_solr": "^4.2",
        "drupal/seckit": "^2.0",
        "drupal/simple_sitemap": "^4.0",
        "drupal/smtp": "^1.0",
        "drupal/superfish": "^1.4",
        "drupal/title_length": "^1.1",
        "drupal/token": "^1.6",
        "drupal/transliterate_filenames": "^1.3",
        "drupal/twig_tweak": "^3.1",
        "drupal/upgrade_status": "^3.12",
        "drupal/variationcache": "^1.0",
        "drupal/views_bootstrap": "^4.3",
        "drupal/views_bulk_edit": "^2.4",
        "drupal/views_bulk_operations": "^4.0",
        "drupal/views_contextual_filters_or": "^1.2",
        "drupal/views_data_export": "^1.0",
        "drupal/webform": "^6.2@beta",
        "drupal/webprofiler": "^9.0",
        "drupal/weight": "^3.3",
        "drupal/workbench": "^1.3",
        "drupal/workbench_email": "^2.0",
        "drush/drush": "^10.6.2",
        "easyrdf/easyrdf": "^1.1",
        "highlightjs/highlightjs": "^9.7",
        "islandora-rdm/islandora_fits": "dev-8.x-1.x",
        "islandora/chullo": "1.x-dev",
        "islandora/controlled_access_terms": "2.x-dev",
        "islandora/islandora": "^2",
        "islandora/islandora_defaults": "2.x-dev",
        "islandora/openseadragon": "2.x-dev",
        "npm-asset/nouislider": "^14",
        "oomphinc/composer-installers-extender": "^2.0",
        "professional-wiki/edtf": "^2.0",
        "vlucas/phpdotenv": "^2.4",
        "webflo/drupal-finder": "^1.0.0",
        "webmozart/path-util": "^2.3",
        "zaporylie/composer-drupal-optimizations": "^1.1"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "cweagans/composer-patches": true,
            "composer/installers": true,
            "drupal/core-composer-scaffold": true,
            "drupal/core-project-message": true,
            "zaporylie/composer-drupal-optimizations": true,
            "drupal/console-extend-plugin": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "oomphinc/composer-installers-extender": true
        }
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": [
            "load.environment.php"
        ]
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "@composer drupal:scaffold",
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "@composer drupal:scaffold",
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library",
                "vendor:npm-asset",
                "vendor:bower-asset"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "web/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "web/profiles/custom/{$name}": [
                "type:drupal-custom-profile"
            ],
            "web/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        },
        "drupal-core-project-message": {
            "include-keys": [
                "homepage",
                "support"
            ],
            "post-create-project-cmd-message": [
                "<bg=blue;fg=white>                                                         </>",
                "<bg=blue;fg=white>  Congratulations, youâ€™ve installed the Drupal codebase  </>",
                "<bg=blue;fg=white>  from the drupal/recommended-project template!          </>",
                "<bg=blue;fg=white>                                                         </>",
                "",
                "<bg=yellow;fg=black>Next steps</>:",
                "  * Install the site: https://www.drupal.org/docs/8/install",
                "  * Read the user guide: https://www.drupal.org/docs/user_guide/en/index.html",
                "  * Get support: https://www.drupal.org/support",
                "  * Get involved with the Drupal community:",
                "      https://www.drupal.org/getting-involved",
                "  * Remove the plugin that prints this message:",
                "      composer remove drupal/core-project-message"
            ]
        },
        "drupal-scaffold": {
            "initial": {
                ".editorconfig": "../.editorconfig",
                ".gitattributes": "../.gitattributes"
            },
            "locations": {
                "web-root": "web/"
            }
        },
        "patches": {
            "drupal/captcha": {
                "fix for permissions": "https://www.drupal.org/files/issues/2021-06-18/captcha-white-image-captcha-3211385-12.patch"
            },
            "drupal/context": {
                "Views integration": "https://www.drupal.org/files/issues/context-views-condition-2938094-6.patch"
            },
            "drupal/core": {
                "fix for taxo term save": "https://www.drupal.org/files/issues/2020-10-29/2893747-33.patch",
                "fix for blocks not showing up": "https://www.drupal.org/files/issues/2018-10-27/2997898-21.patch",
                "fix view all PHP 8 error": "https://www.drupal.org/files/issues/2022-03-14/3266341-9.patch",
                "fix for large taxonomies": "https://www.drupal.org/files/issues/2021-05-26/763380-73.patch"
            },
            "drupal/google_analytics": {
                "fix for GA version 4": "https://www.drupal.org/files/issues/2020-12-01/3178179-google-analytics-4.patch"
            },
            "drupal/facets": {
                "nouislider fix": "https://www.drupal.org/files/issues/2021-06-16/issue-3153622-nouislider_4.patch"
            },
            "drupal/metatag": {
                "fix for metatag delimiter": "https://git.drupalcode.org/project/metatag/-/merge_requests/20.diff"
            },
            "drupal/migrate_source_ui": {
                "allow url as a migrate source": "https://raw.githubusercontent.com/asulibraries/islandora-repo/develop/web/modules/custom/asu_migrate/source_ui_url.patch"
            },
            "drupal/rest_oai_pmh": {
                "fix for empty values and incorrect namespaces": "https://raw.githubusercontent.com/asulibraries/islandora-repo/develop/web/modules/custom/asu_custom_rdf/ns_and_empty_fixes.patch"
            },
            "drupal/views_data_export": {
                "fix for batch export": "https://www.drupal.org/files/issues/2021-02-12/3173296-9-batch-query-alter.patch"
            },
	    "islandora/jsonld": {
	        "fix for broken target_id": "https://patch-diff.githubusercontent.com/raw/Islandora/jsonld/pull/68.patch"
	    }
        },
        "installer-types": [
            "npm-asset",
            "bower-asset"
        ]
    },
    "require-dev": {
        "phpspec/prophecy-phpunit": "^2.0",
        "phpunit/phpunit": "^9.5",
        "symfony/phpunit-bridge": "^5.2"
    }
}
